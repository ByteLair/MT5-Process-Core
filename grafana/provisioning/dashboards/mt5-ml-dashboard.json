{
  "annotations": {"list": []},
  "editable": true,
  "panels": [
    {
      "datasource": {"type": "loki", "uid": "loki"},
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "showContext": true,
        "limit": 1000
      },
      "targets": [
        {"expr": "{job=\"ml\"}", "refId": "A"}
      ],
      "title": "ML Worker Logs",
      "type": "logs"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "id": 2,
      "options": {"colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""}, "textMode": "auto"},
      "targets": [{"expr": "ml_train_jobs_total", "refId": "A"}],
      "title": "Total ML Train Jobs",
      "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "id": 3,
      "options": {"colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""}, "textMode": "auto"},
      "targets": [{"expr": "ml_train_jobs_failed_total", "refId": "A"}],
      "title": "Failed ML Train Jobs",
      "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "id": 4,
      "options": {"legend": {"calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true}, "tooltip": {"mode": "multi", "sort": "none"}},
      "targets": [{"expr": "rate(ml_train_jobs_total[$__rate_interval])", "refId": "A"}],
      "title": "ML Train Jobs Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "id": 5,
      "options": {"legend": {"calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true}, "tooltip": {"mode": "multi", "sort": "none"}},
      "targets": [{"expr": "rate(ml_train_jobs_failed_total[$__rate_interval])", "refId": "A"}],
      "title": "ML Train Jobs Failed Rate",
      "type": "timeseries"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["ml", "ai", "training", "logs"],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"},
  "timepicker": {},
  "timezone": "",
  "title": "MT5 ML/AI Dashboard",
  "uid": "mt5-ml-dashboard",
  "version": 1
}
